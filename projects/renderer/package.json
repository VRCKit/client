{
  "name": "vrckit",
  "private": true,
  "version": "0.3.5.1",
  "description": "VRCKit",
  "displayName": "VRCKit",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "analyze": "npm ls --depth=0 --long",
    "clean": "rimraf out node_modules/.cache"
  },
  "keywords": [],
  "author": {
    "name": "Kıraç Armağan Önal",
    "email": "kiracarmaganonal@gmail.com"
  },
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/VRCKit/client",
  "repository": {
    "type": "git",
    "url": "https://github.com/VRCKit/client.git"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/icon",
        "name": "VRCKit",
        "executableName": "VRCKit",
        "asar": true,
        "prune": true,
        "ignore": [
          "/\\.git($|/)",
          "/node_modules/\\.cache($|/)",
          "/\\.vscode($|/)",
          "/\\.github($|/)",
          "/\\.gitignore$",
          "/README\\.md$",
          "/\\.eslintrc.*$",
          "/\\.prettierrc.*$",
          "/tsconfig\\.json$",
          "/src/.*\\.ts$",
          "/\\.env.*$",
          "/docs($|/)",
          "/test($|/)",
          "/tests($|/)",
          "/\\.nyc_output($|/)",
          "/coverage($|/)",
          "/\\*\\.log$",
          "/\\.DS_Store$",
          "/Thumbs\\.db$"
        ],
        "win32metadata": {
          "CompanyName": "VRCKit",
          "FileDescription": "VRCKit",
          "ProductName": "VRCKit"
        }
      },
      "rebuildConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "vrckit",
            "authors": "Kıraç Armağan Önal",
            "description": "VRCKit: Your Ultimate AI VRChat Avatar Explorer, Discovery Platform & Customization Hub",
            "setupIcon": "assets/icon.ico",
            "loadingGif": "assets/loading.png",
            "noMsi": true
          }
        }
      ],
      "publishers": [],
      "plugins": []
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.1",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "json-stream": "^1.0.0",
    "line-reader": "^0.4.0",
    "mime-types": "^3.0.1",
    "original-fs": "^1.2.0",
    "osc": "^2.4.5",
    "puppeteer-core": "^24.15.0",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron/packager": "^18.4.0",
    "@types/line-reader": "^0.0.37",
    "@types/node": "^22.9.0",
    "@types/ws": "^8.5.13",
    "electron": "^34.0.2"
  }
}